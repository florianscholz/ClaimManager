@model List<ClaimManager.DataLayer.Models.LossType>
@{
    ViewData["Title"] = "Loss Types";
}

<div class="text-center">
    <h1 class="display-4">LossTypes</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Code</th>
          <th scope="col">Description</th>
        </tr>
      </thead>
      <tbody>
      @foreach (var lossType in Model)
      {
        <tr>
          <th scope="row">@lossType.Id</th>
          <td>@lossType.Code</td>
          <td>@lossType.Description</td>
        </tr>
      }
      </tbody>
    </table>
</div>